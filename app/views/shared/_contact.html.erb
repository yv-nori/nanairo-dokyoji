<div class="L_back-para">
  <div class="C_box__side-bar-inner pc-tab-only"></div>
  <section class="PageTitle">
    <div id="JS_smoothScroll_target-0"></div>
    <div class="L_pageTitle__headding">
      <div class="">
        <p class="C_text__title-en">
          Contact
        </p>
        <h1 class="C_text__title-ja">
          お問い合わせ
        </h1>
      </div>
    </div>
  </section>
</div>
<section class="PageKeyVisual" id="JS_scroll-to-top_trigger-0">
  <div class="L_kv-half-para">
    <picture>
      <source data-srcset="https://dazq5oer2h0ku.cloudfront.net/images/contact/kv_pc.webp" media="(min-width: 1025px)" type="image/webp" class="C_image__para"/>
      <source data-srcset="https://dazq5oer2h0ku.cloudfront.net/images/contact/kv_tab.webp" media="(min-width: 600px)" type="image/webp" class="C_image__para"/>
      <source data-srcset="https://dazq5oer2h0ku.cloudfront.net/images/contact/kv_sp.webp 1x, https://dazq5oer2h0ku.cloudfront.net/images/contact/kv_sp2.webp 2x" type="image/webp" class="C_image__para"/>
      <img src="https://dazq5oer2h0ku.cloudfront.net/images/contact/kv_dummy.webp" data-src="https://dazq5oer2h0ku.cloudfront.net/images/contact/kv_alt.jpg" alt="お問い合わせ 桜" class="C_image__para lazyload"/>
    </picture>
  </div>
</section>
<div class="L_back-para">
  <div class="C_box__side-bar-inner pc-tab-only"></div>
  <section class="Contact" id="return">
    <div class="L_wrap__600">
      <%=render partial:'shared/form', collection: form_items ,as:'form_items' %>
    </div>
  </section>
</div>
<%= render 'shared/side-bar',{ color: '' }%>
