<section class="Recruit"  id="JS_scroll-to-top_trigger-2">
  <div id="JS_scroll-action-14_trigger">
    <div class="P_Pa__recruit">
      <svg class="P_Pa__recruit__deco-line" x="0px" y="0px" width="100%" height="100%">
        <line class="P_Pa__recruit__deco-line-1" x1="0" y1="2%" x2="100%" y2="2%"/>
        <line class="P_Pa__recruit__deco-line-2" x1="0" y1="4%" x2="100%" y2="4%"/>
