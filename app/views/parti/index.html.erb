<section class="Nav">
  <div class="P_Pa__nav__wrap">
    <div class="P_Pa__nav__top-wrap">
      <div class="P_Pa__nav__belong-box" id="JS_scroll-action-9_trigger">
        <div class="P_Pa__nav__rain">
          <div class="C_deco__rain"></div>
        </div>
        <div class="P_Pa__nav__title-box left">
          <div class="P_Pa__nav__title-ja">在園の方へ
            <svg class="P_Pa__nav__deco-under-line" width="110" height="12" id="JS_scroll-action-9_target-0">
              <polyline points="1,10 95,10 90,5" fill="transparent"/>
            </svg>
          </div>
          <div class="P_Pa__nav__read">READ MORE</div>
          <%= link_to "",  belong_parti_index_path, class:"U_link"%>
        </div>
        <div class="P_Pa__nav__img-box">
          <div class="P_Pa__nav__trim-img">
            <div class="C_image__belong__banner"></div>
          </div>
          <div class="P_Pa__nav__deco-top-left"></div>
          <div class="P_Pa__nav__deco-top-right"></div>
          <div class="P_Pa__nav__deco-bottom-left"></div>
          <div class="P_Pa__nav__deco-bottom-right"></div>
        </div>
      </div>
      <svg class="P_Pa__nav__deco-line-ver pc-tab-only" x="0px" y="0px" width="1px" height="120%">
        <line x1="0" y1="0.5" x2="1" y2="100%"/>
      </svg>
      <div class="P_Pa__nav__admission-box" id="JS_scroll-action-10_trigger">
        <div class="P_Pa__nav__birds">
          <div class="C_deco__birds"></div>
        </div>
        <div class="P_Pa__nav__title-box right">
          <div class="P_Pa__nav__title-ja">入園希望の方へ
            <svg class="P_Pa__nav__deco-under-line right" width="110" height="12" id="JS_scroll-action-10_target-0">
              <polyline points="1,10 95,10 90,5" fill="transparent"/>
            </svg>
          </div>
          <div class="P_Pa__nav__read">READ MORE</div>
          <%= link_to "",  admission_parti_index_path, class:"U_link"%>
        </div>
        <div class="P_Pa__nav__img-box admission">
          <div class="P_Pa__nav__trim-img">
            <div class="C_image__admission__banner"></div>
          </div>
          <div class="P_Pa__nav__deco-top-left"></div>
          <div class="P_Pa__nav__deco-top-right"></div>
          <div class="P_Pa__nav__deco-bottom-left"></div>
          <div class="P_Pa__nav__deco-bottom-right"></div>
        </div>
      </div>
    </div>
    <div class="P_Pa__nav__under-wrap">
      <div class="P_Pa__nav__access-title-box" id="JS_scroll-action-11_target-0">
        <div id="JS_scroll-action-11_trigger" style="height:100%;">
          <div class="P_Pa__nav__under-title-en">ACCESS</div>
          <div class="P_Pa__nav__under-title-ja">アクセス</div>
