        <line class="P_Pa__recruit__deco-line-1" x1="0" y1="94%" x2="100%" y2="94%"/>
        <line class="P_Pa__recruit__deco-line-2" x1="0" y1="96%" x2="100%" y2="96%"/>
        <line class="P_Pa__recruit__deco-line-1" x1="0" y1="98%" x2="100%" y2="98%"/>
