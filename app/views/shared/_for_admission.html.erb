<div id="JS_smoothScroll_target-0" style="top:0;position:absolute;"></div>
<section class="P_for-admission">
  <div class="P_for-admission__kv" id="JS_scroll-to-top_trigger-0">
    <picture>
      <source data-srcset="https://dazq5oer2h0ku.cloudfront.net/images/for-admission/kv_pc.webp" media="(min-width: 1025px)" type="image/webp" class="C_image"/>
      <source data-srcset="https://dazq5oer2h0ku.cloudfront.net/images/for-admission/kv_tab.webp" media="(min-width: 600px)" type="image/webp" class="C_image"/>
      <source data-srcset="https://dazq5oer2h0ku.cloudfront.net/images/for-admission/kv_sp.webp 1x, https://dazq5oer2h0ku.cloudfront.net/images/for-admission/kv_sp2.webp 2x" type="image/webp" class="C_image"/>
      <img src="https://dazq5oer2h0ku.cloudfront.net/images/for-admission/kv_dummy.webp" data-src="https://dazq5oer2h0ku.cloudfront.net/images/for-admission/kv_alt.jpg" alt="入園にむけて トップ画像" class="C_image lazyload"/>
    </picture>
    <div class="P_for-admission__page-title-en">
      ADMISSION
      <div class="P_for-admission__font-deco left">
        <div class="C_deco__text-shine3"></div>
      </div>
      <div class="P_for-admission__font-deco right">
        <div class="C_deco__text-shine4"></div>
      </div>
    </div>
    <div class="P_for-admission__page-title-ja">
      入園に向けて
    </div>
  </div>
  <div class="P_for-admission__wrap" id="movie">
    <div class="P_for-admission__container first">
      <div class="P_for-admission__content-title">
        入園に向けて　幼児編
        <div class="P_for-admission__deco title-deco">
          <div class="C_deco__jagged1"></div>
        </div>
      </div>
      <div class="P_for-admission__content-img">
        <iframe width="100%" height="100%" src="<%= items[:toddler_link] %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen class="P_for-admission__youtube-frame"></iframe>
        <div class="P_for-admission__deco img-deco">
          <div class="C_deco__dots1"></div>
        </div>
      </div>
    </div>
    <div class="P_for-admission__container">
      <div class="P_for-admission__content-title">
        入園に向けて　乳児編
        <div class="P_for-admission__deco title-deco">
          <div class="C_deco__babble1"></div>
        </div>
      </div>
      <div class="P_for-admission__content-img">
        <iframe width="100%" height="100%" src="<%= items[:infant_link] %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen class="P_for-admission__youtube-frame"></iframe>
        <div class="P_for-admission__deco img-deco">
          <div class="C_deco__triangle1"></div>
        </div>
      </div>
    </div>
  </div>
</section>
<%= render 'shared/side-bar_noBack',{color: ''}%>
