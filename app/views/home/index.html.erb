<%= render partial: 'layouts/header' %>
<section class="top-section">
  <div class="top_image_box">
    <div class="top_image slide1 order1"></div>
    <div class="top_image slide2 order1"></div>
    <div class="top_image slide3 order1"></div>
    <div class="top_image slide4 order1"></div>
    <div class="top_image slide5 order1"></div>
    <div class="text">
      <p class="sub_title">Educational Principle</p>
      <p class="title">遊　学　優　和</p>
    </div>
    <div class="mouse js-hide">
        <span></span>
    </div>
    <div class="change-slide-container right js-hide">
      <button class="js-click-slide-right">
        <i class="fas fa-chevron-right"></i>
      </button>
    </div>
    <div class="change-slide-container left js-hide">
      <button class="js-click-slide-left">
        <i class="fas fa-chevron-left"></i>
      </button>
    </div>
  </div>
  <div class="catch-copy-container">
  </div>
</section>
<section class="main-section">
  <div class="preschool__wrapper">
    <div class="preschool__item">
      <div class="preschool__image">
        <picture class="responsive-image">
          <source srcset='/images/parti.png' data-width="710" data-height="425" media=<%= Settings.media[:pc] %>>
          <source srcset='/images/parti.png' data-width="710" data-height="425" media="(min-width: 640px)">
          <source srcset='/images/parti.png' data-width="325" data-height="195" media="(min-width: 1px)">
          <%= image_tag '/images/parti.png', class: "parti-responsive-image lazyloaded" %>
        </picture>
      </div>
      <div class="preschool__content">
        <h3>パルティ</h3>
        <div class="preschool__link">
          <%= link_to "", root_path, class: "action" %>
        </div>
      </div>
    </div>
    <div class="preschool__item">
      <div class="preschool__image">
        <picture class="responsive-image">
          <source srcset='/images/piani.png' data-width="710" data-height="425" media="(min-width: 960px)">
          <source srcset='/images/piani.png' data-width="710" data-height="425" media="(min-width: 640px)">
          <source srcset='/images/piani.png' data-width="325" data-height="195" media="(min-width: 1px)">
          <%= image_tag '/images/piani.png', class: "responsive-image lazyloaded" %>
        </picture>
      </div>
      <div class="preschool__content">
        <h3>ピアニィ</h3>
        <div class="preschool__link">
          <%= link_to "", root_path, class: "action" %>
        </div>
      </div>
    </div>
  </div>
</section>

